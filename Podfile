# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

target 'SocialTest' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SocialTest
  pod 'Google/SignIn'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'TwitterKit'
  pod 'TwitterCore'

  target 'SocialTestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SocialTestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
